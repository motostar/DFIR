#############################################################################################################################
# 
# Splunk_NGINX_Investigator.xml
#
#  .SYNOPSIS
#     Splunk Dashboard for NGINX Accesslog investigation
#
#  .DESCRIPTION
#.      This XML produces interactive Splunk dashboard for NGINX investigation Specifically for combined logformat. 
#
#  .PREREQUISITES
#       1. Splunk Admin access for enabling dash
#       2. Necessary license and log samples
#
#   .NOTES
#.      Author: Balasubramanya Chandrashekar
#       Date: 2023-01-16
#       Version: 1.0
############################################################################################################################

<form version="1.1" theme="dark">
  <label>Nginx investigator</label>
  <search id="base_search">
    <query>index="nginx" sourcetype="access_combined"
    | iplocation clientip
    | table _time bytes clientip Country file ident method referer req_time status uri uri_path useragent version
    </query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  <fieldset submitButton="false">
    <input type="time" token="time" searchWhenChanged="true">
      <label>TimePicker</label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="dropdown" token="Region" searchWhenChanged="true">
      <label>Access Region</label>
      <fieldForLabel>Country</fieldForLabel>
      <fieldForValue>Country</fieldForValue>
      <search base="base_search">
        <query>|iplocation clientip
|stats count by Country</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="useragent" searchWhenChanged="true">
      <label>UserAgent</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>useragent</fieldForLabel>
      <fieldForValue>useragent</fieldForValue>
      <search base="base_search">
        <query>|search useragent!=-
|stats count by useragent</query>
      </search>
    </input>
    <input type="dropdown" token="status" searchWhenChanged="true">
      <label>status</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>status</fieldForLabel>
      <fieldForValue>status</fieldForValue>
      <search base="base_search">
        <query>|stats count by status</query>
      </search>
    </input>
  </fieldset>
  <row>
    <panel id="panel_layout">
      <input id="input_link_split_by" type="link" token="unused" searchWhenChanged="true">
        <label>Investigation Selector</label>
        <choice value="operationsSummary">operationsSummary</choice>
        <choice value="suspicious">suspicious</choice>
        <choice value="sigmamatch">sigmamatch</choice>
        <choice value="vul">vul</choice>
        <default>operationsSummary</default>
        <change>
          <condition value="operationsSummary">
            <set token="operationsSummary">true</set>
            <unset token="sigmamatch"></unset>
            <unset token="suspicious"></unset>
            <unset token="vul"></unset>
          </condition>
          <condition value="sigmamatch">
            <set token="sigmamatch">true</set>
            <unset token="operationsSummary"></unset>
            <unset token="suspicious"></unset>
            <unset token="vul"></unset>
          </condition>
          <condition value="suspicious">
            <set token="suspicious">true</set>
            <unset token="operationsSummary"></unset>
            <unset token="sigmamatch"></unset>
            <unset token="vul"></unset>
          </condition>
          <condition value="vul">
            <set token="vul">true</set>
            <unset token="operationsSummary"></unset>
            <unset token="sigmamatch"></unset>
            <unset token="suspicious"></unset>
          </condition>
        </change>
      </input>
      <html>
        <style>
          /* This Layout Panel Bottom Padding removed to merge Link Input with horizontal line */
          #panel_layout .fieldset{
            padding: 10px 12px 0px 12px !important;          
          }
          /* Increase width of Link input to have options in Single Line */
          #input_link_split_by.input-link{
            width: 2040px !important;
          }
          /* Change from flex to -webkit-box for side by side layout */
          #input_radio_split_by.input-link div[data-component="splunk-core:/splunkjs/mvc/components/LinkList"]{
            display: -webkit-box !important;
          }
          /* Create Button Border to make them appear as Tabs */
          #input_link_split_by.input-link button{
            width: 130px !important;
            border-top-color: rgb(255, 255, 255);
            border-top-style: solid;
            border-top-width: 1px;
            border-right-color: rgb(255, 255, 255);
            border-right-style: solid;
            border-right-width: 1px;
            border-left-color: rgb(255, 255, 255);
            border-left-style: solid;
            border-left-width: 1px;
            border-top-left-radius: 10px;
            border-top-right-radius: 10px;
          }
          /* Hide link input bottom message section to merge with Horizontal line */
          .dashboard-panel #input_link_split_by label,
          #input_link_split_by .splunk-choice-input-message{
            display: none !important;
          }
          /* Remove padding from horizontal line */
          #panel_layout .panel-body.html{
            padding: 0px !important;
          }
        </style>
        <hr style="height:1px;border-width:0;color: black;background-color: white;margin: 0px;"/>
      </html>
    </panel>
  </row>
  <row>
    <panel id="ipanel_layout">
      <html depends="$alwaysHideCSSOverrideForDashboard$">
        <style>
          #panel_chart_line_error_trend{
            width: 70% !important;
          }
          #panel_chart_pie_error_split{
            width: 30% !important;
          }
          /* Font Size Color Adjustment for Series Compare in Chart Legend */
          div#chart_line_error_trending svg g.highcharts-legend g.highcharts-legend-item text tspan:nth-child(2){
             font-size: 120% !important;
             fill: cyan !important;
             font-weight: bold !important;
          }
          /* Trellis Layout CSS */
          #single_snapshot #facet-viz_groupby_field_log_level_groupby_value_INFO rect{
            fill: #006D9C !important;
          }
          #single_snapshot #facet-viz_groupby_field_log_level_groupby_value_WARN rect{
            fill: #F8BE34 !important;
          }
          #single_snapshot #facet-viz_groupby_field_log_level_groupby_value_WARNING rect{
            fill: #F8BE34 !important;
          }
          #single_snapshot #facet-viz_groupby_field_log_level_groupby_value_ERROR rect{
            fill: #DC4E41 !important;
          }
        </style>
      </html>
    </panel>
  </row>
  <row depends="$operationsSummary$">
    <panel>
      <chart>
        <title>Access Summary by Req Method</title>
        <search base="base_search">
          <query>|search Country="$Region$" useragent="$useragent$" status="$status$"
|timechart span=1MON count by method</query>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Access Summary Resp Status</title>
        <search base="base_search">
          <query>|search Country="$Region$" useragent="$useragent$" status="$status$"
|timechart span=1MON count by status</query>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row depends="$operationsSummary$">
    <panel>
      <map>
        <title>Web Access by Regions</title>
        <search base="base_search">
          <query>| iplocation clientip
| search Country="$Region$" useragent="$useragent$" status="$status$"
| geostats count by Country</query>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.type">marker</option>
        <option name="refresh.display">progressbar</option>
      </map>
    </panel>
    <panel>
      <map>
        <title>Failed Web Access by Regions</title>
        <search base="base_search">
          <query>| iplocation clientip
| search Country="$Region$" useragent="$useragent$"status!=200 status="$status$"
| geostats count by status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.type">marker</option>
        <option name="refresh.display">progressbar</option>
      </map>
    </panel>
  </row>
  <row depends="$operationsSummary$">
    <panel>
      <table>
        <title>Error Stats</title>
        <search base="base_search">
          <query>| eval clientError=if(match(status, "^4\\d{2}"), 1, 0), serverError=if(match(status, "^5\\d{2}"), 1, 0), redirection=if(match(status, "^3\\d{2}"), 1, 0)
| search clientError=1 OR serverError=1 OR redirection=1 Country="$Region$" useragent="$useragent$" status="$status$"
| stats count as hits, dc(clientip) as UniqueHits, sum(redirection) as HTTP_3XX, sum(clientError) as HTTP_4XX, sum(serverError) as HTTP_5XX, sum(bytes) as totalbytes by uri
|sort - hits</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>UserAgent Summary - Rare</title>
        <search base="base_search">
          <query>| search Country="$Region$" useragent="$useragent$" useragent!=- status="$status$"
| rex field=useragent "(?P&lt;Useagent&gt;[^\/]+).*"
| stats count by Useagent
| rare limit=15 Useagent</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row depends="$operationsSummary$">
    <panel>
      <table>
        <title>unusual remote_ips - high request rates with errors</title>
        <search base="base_search">
          <query>|search status!=2** clientip!=10.*.*.* Country="$Region$" useragent="$useragent$" status="$status$"
| eval ip_and_country=clientip." - ".Country
|stats count as hits, dc(uri) as URIHits, values(status) as response by ip_and_country
| table ip_and_country URIHits response hits
| sort - hits</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <format type="color" field="hits">
          <colorPalette type="minMidMax" maxColor="#D41F1F" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
      </table>
    </panel>
    <panel>
      <table>
        <title>referers observed from logs</title>
        <search base="base_search">
          <query>| search referer!="-" Country="$Region$" useragent="$useragent$" status="$status$"
| iplocation clientip
| eval ip_and_country=clientip." - ".Country
| stats count by ip_and_country referer</query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
  <row depends="$sigmamatch$">
    <panel>
      <table>
        <title>Webshell string observed in URI</title>
        <search base="base_search">
          <query>| search uri IN ("*whoami*", "*net%20user*", "*net+user*", "*net%2Buser*", "*cmd%20/c%*", "*cmd+/c+*", "*cmd%2B/c%*", "*cmd%20/r%*", "*cmd+/r+*", "*cmd%2B/r%*", "*cmd%20/k%*", "*cmd+/k+*", "*cmd%2B/k%*", "*powershell%*", "*powershell+*", "*tasklist%*", "*tasklist+*", "*wmic%*", "*wmic+*", "*ssh%*", "*ssh+*", "*python%*", "*python+*", "*python3%*", "*python3+*", "*ipconfig*", "*wget%*", "*wget+*", "*curl%*", "*curl+*", "*certutil*", "*copy%20%5C%5C*", "*dsquery%*", "*dsquery+*", "*nltest%*", "*nltest+*",| search url IN ("*wso.php*", "*weevely.php*", "*simple-shell.php*", "*sadrazam.php*", "*r57.php*", "*p0wny-shell.php*", "*mini.php*","*marion001.php*","*indoxploit.php*", "*b374k.php*", "*alfa.php*", "*ak47shell.php*", "*0byt3m1n1.php*","*pouya.asp*", "*newaspcmd.asp*","*kacak.asp*","*aspcmd.asp*","*.Aar*","*.Ascx*","*.Ashx*","*.Asmx*","*.Asp*","*.Aspx*","*.Cfm*","*.Cgi*","*.Javascript*","*.Jsp*","*.Jspx*","*.LICENSE*","*.Mysql*","*.Nginx*","*.Other*","*.Pl*","*.README.md*","*.SSH*","*.Soap*","*.Udp*","*.WeBaCoo*","*.gdog*","*.icmp*","*.jar*","*.nodejs*","*.openfire*","*.osx*","*.pwnginx*","*.python*","*.reGeorg-master*","*.ruby*","*.servlet*","*.sh*","*.war*","*.xml*","*.xssshell*")
| table clientip Country uri status)
| iplocation clientip
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>XSS string observed in URI</title>
        <search base="base_search">
          <query>| search uri IN ("*&lt;script&gt;*", "*%3Cscript%3E*", "*%253Cscript%253E*","*&lt;iframe *","*%3Ciframe *","*&lt;svg *","*%3Csvg *","*document.cookie*","*document.domain*","* onerror=*","* onresize=*","* onload=*","*onmouseover=*","*${alert*","*javascript:alert*","*javascript%3Aalert*","*cscript*","*wscript*","*src=*","*href=*","*@import*","*STYLE=*","*javascript:prompt*","*iframe*","*onstart=*","*alert\(*","*&amp;lt;script&amp;gt;*")
| iplocation clientip
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <format type="color" field="status">
          <colorPalette type="map">{"200":#D41F1F}</colorPalette>
        </format>
      </table>
    </panel>
  </row>
  <row depends="$sigmamatch$">
    <panel>
      <table>
        <title>SQL Injection Strings</title>
        <search base="base_search">
          <query>| search uri IN ("*=select *","*=select%20*","*=select(*","*UNION SELECT*","*UNION%20SELECT*","*UNION ALL SELECT*","*UNION%20ALL%20SELECT*","*CONCAT(0x*","*order by *","*order%20by%20*","*information_schema.tables*","*group_concat(*","*table_schema*","*select%28sleep%2810%29*","*@@version*","*'1'='1*","*%271%27%3D%271*","*SELECTCHAR(*","*select * *","*select%20*%20*","*or 1=1#*","*or%201=1#*")
| iplocation clientip
| table clientip Country uri useragent status</query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Possibel URL redirection attempt</title>
        <search base="base_search">
          <query>| search uri IN ("*?next=*","*?rurl=*","*?dest=*","*?destination=*","*?redir=*","*?redirect_uri=*","*?redirect_url=*","*?redirect=*","*/redirect/*","*/cgi-bin/redirect.cgi?*","*/out/*","*/out?*","*?view=*","*/login?to=*","*?image_url=*","*?go=*","*?return=*","*?returnTo=*","*?return_to=*","*?checkout_url=*","*?continue=*","*?return_path=*","*=http*","*@*") 
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row depends="$sigmamatch$">
    <panel>
      <table>
        <title>Webshell ReGeorg Detection</title>
        <search base="base_search">
          <query>| search uri IN ("*cmd=read*","*connect&amp;target*","*cmd=connect*","*cmd=disconnect*","*cmd=forward*")
| iplocation clientip
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Server Side Template Injection Strings</title>
        <search base="base_search">
          <query>| search uri IN ("*={{*","*%7B%7B*","*=&lt;%=*", "*=%3C%25=*","*=@(*","*freemarker.template.utility.Execute*","*.getClass().forName('javax.script.ScriptEngineManager*","*org.apache.commons.io.IOUtils*")
| iplocation clientip
| table clientip Country uri useragent status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row depends="$sigmamatch$">
    <panel>
      <table>
        <title>Path Traversal Exploitation Attempts</title>
        <search base="base_search">
          <query>| search uri IN ("*/etc/passwd*","*/windows*","*/lib/passwd*","*/etc/*","/*/windows/*")
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>OS Commands in URI</title>
        <search base="base_search">
          <query>| search uri IN ("*whoami*","*ls -*","*ps -*","*chmod*","*chown*","*nano*","*touch*","*make*","*install*","*nslookup*","*rm *","*echo*","*uname*","*netstat*","*ifconfig*","*tracert*","*ssh*","*scp*","*ftp*","*telnet*","*tftp*","*ping *","*bash*","*cron*","*wtmp*","*btmp*","*php-ping*","*php_ping*","*cat%20*","*netsh*","*gcc*","*netcat*","*perl*","*python*","*which*","*wget*","*curl*","*print*","*type*","*awk*","*sed*","*cmd=ls+-la*", "*reg add*", "*eval*", "*nc -*", "*pwd*", "*shell_exec*" )
| table clientip Country uri status</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row depends="$suspicious$">
    <panel>
      <table>
        <title>Multiple Suspicious Resp Codes Caused by Single Client</title>
        <search base="base_search">
          <query>| eval ip_and_country=clientip." - ".Country
| stats count as hits values(status) as response values(uri) as urlAccessed by ip_and_country
| search response!=200 hits&gt;10
| sort - hits</query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Multiple Uncommmon http methods by single client</title>
        <search base="base_search">
          <query>| search NOT method IN ("GET","POST")
| eval ip_and_country=clientip." - ".Country
| stats count as hits values(method) as method by ip_and_country
| sort - hits</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row depends="$suspicious$">
    <panel>
      <table>
        <title>Known bad Useragent</title>
